#:import ScreenManager kivy.uix.screenmanager 
#:import MDScreen kivymd.uix.screen 

ScreenManager:
    id: sm
	MainScreen:
	LogScreen:


<LogScreen@MDScreen>:
    name: "log"
    # md_bg_color: self.theme_cls.backgroundColor
    md_bg_color: 0.4941,0.3019,0.4863,1
    
    MDBoxLayout:
        orientation: "vertical"
        
        MDLabel:
            text:     'File Manager'
            halign: 'center'
            pos_hint: {"center_x": 0.5 ,"center_y": 0.94}
            theme_font_size: "Custom"
            font_size: "48dp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            theme_font_name: "Custom"
            font_name: "assets/fonts/Anton-Regular.ttf"
        
        MDLabel:
            text:     'Modified' # 
            halign: 'center'
            pos_hint: {"center_x": 0.5 ,"center_y": 0.88}
            theme_font_size: "Custom"
            font_size: "25dp"
            theme_text_color: 'Custom'
            text_color: 1,1,1,1
            theme_font_name: "Custom"
            font_name: "assets/fonts/Anton-Regular.ttf"
        
        FitImage:
            source: "assets/images/logo.png"
            size_hint : [None,None]
            width : "40sp"
            height : "40sp"
            pos_hint: {"x": 0.02, "y": 0.02}
            # radius: self.width / 2

        MDLabel:
            id : log1
            text : "Log appear here"
            pos_hint: {"x": .1, "y": .25}
            bold: True
            text_color : "white" #self.theme_cls.primaryColor
            theme_font_name: "Custom"
            font_name: "assets/fonts/DMSerifDisplay-Regular.ttf"
            text_color : 1,1,1,1
            theme_font_size: "Custom"
            font_size: "24dp"
        MDLabel:
            id : log2
            text : "Log1"
            pos_hint: {"x": .2, "top": .018}
            bold: True
            text_color : "white" #self.theme_cls.primaryColor
            theme_font_name: "Custom"
            font_name: "assets/fonts/DMSerifDisplay-Regular.ttf"
            text_color : 1,1,1,1
            theme_font_size: "Custom"
            font_size: "20dp"
            valign : "bottom"
        MDLabel:
            id : log3
            text : "Log2"
            pos_hint: {"x": .1, "y": .1}
            bold: True
            text_color : "white" #self.theme_cls.primaryColor
            theme_font_name: "Custom"
            font_name: "assets/fonts/DMSerifDisplay-Regular.ttf"
            text_color : 1,1,1,1
            theme_font_size: "Custom"
            font_size: "20dp"
        MDLabel:
            id : log4
            text : "Log3"
            pos_hint: {"x": .2, "y": 0.04}
            bold: True
            text_color : "white" #self.theme_cls.primaryColor
            theme_font_name: "Custom"
            font_name: "assets/fonts/DMSerifDisplay-Regular.ttf"
            text_color : 1,1,1,1
            theme_font_size: "Custom"
            font_size: "20dp"
        MDButton:
            id : otpbutton
            style: "elevated"
            pos_hint: {"center_x": .5, "center_y": 0.18}
            # theme_bg_color: "Custom"
            # md_bg_color: "brown"
            ripple_scale: 0
            on_press : app.backtomain()
            
            MDButtonText:
                text: "Back"

<MainScreen@MDScreen>:
    name: "main"
    # md_bg_color: self.theme_cls.backgroundColor
    md_bg_color: 0.4941,0.3019,0.4863,1
    
